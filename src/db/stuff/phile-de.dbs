		<procedure name="build_patch_new_artifact" id="Procedure_5a5a588" isSystem="false" params_known="y" >
			<string><![CDATA[CREATE OR REPLACE FUNCTION pde.build_patch_new_artifact(_minor_id bigint, _artifact_type_id bigint, _schema_id bigint, _name text)
 RETURNS pde.patch
 LANGUAGE plpgsql
 STRICT SECURITY DEFINER
AS $function$
DECLARE
  _revision integer;
  _artifact_type pde._artifact_type;
  _minor pde.minor;
  _schema pde.schema;
